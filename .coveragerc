[run]
branch = True
source =
    ./config
    ./errors
    ./keys
    ./storageManager
    ./userInterface
    ./utils

[report]
exclude_lines =
    if self.debug:
    pragma: no cover
    raise NotImplementedError
    if __name__ == .__main__.:
ignore_errors = True
omit =
    tests/*

[xml]
output = coverage/reports/coverage.xml
